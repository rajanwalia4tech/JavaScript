{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to Chrome",
			"port": 9222,
			"request": "attach",
			"type": "pwa-chrome",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "pwa-chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}"
		},
		{
			"type": "node-terminal",
			"name": "JavaScript Debug Terminal",
			"request": "launch",
			"cwd": "${workspaceFolder}"
		}
	]
}